{
  "name": "chautauqua-backend",
  "version": "1.0.0",
  "description": "Lambda functions for Chautauqua calendar generation",
  "main": "index.js",
  "scripts": {
    "build": "esbuild src/handlers/*.ts --bundle --platform=node --target=node18 --outdir=dist --external:aws-sdk",
    "dev": "node dist/server.js",
    "deploy": "npm run build && zip -r lambda-function.zip dist/ package.json && aws lambda update-function-code --function-name chautauqua-calendar-generator --zip-file fileb://lambda-function.zip",
    "package": "npm run build && zip -r lambda-function.zip dist/ package.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sync": "ts-node src/scripts/syncData.ts",
    "init-tables": "ts-node src/scripts/initTables.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "express": "^4.18.2",
    "ical-generator": "^4.1.0",
    "node-ical": "^0.20.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "esbuild": "^0.25.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0"
  }
}
